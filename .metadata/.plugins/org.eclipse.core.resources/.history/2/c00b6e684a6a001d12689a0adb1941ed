public class Main {
	
	public static void main(String[] args) {
		
		Parent parent = new Parent();
		parent.parentMethod();
		//this will call the method of parent class.
		
		
		Parent parentchild = new Child();
		parent.parentMethod();
		//here Parent class refernce varible holds a object of child class hence
		//this will call the overriden method of parent class.
	}

}
